{
  "name": "flipboxdigital/relay-salesforce",
  "description": "Salesforce HTTP Relay",
  "version": "1.0.1",
  "keywords": [
    "relay",
    "flipbox",
    "psr-7",
    "salesforce"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/relay-stash": "~1.2",
    "guzzlehttp/guzzle":"~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Relay\\Salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Relay\\Salesforce\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}